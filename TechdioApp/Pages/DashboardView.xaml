<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TechdioApp.Pages.DashboardView"
             xmlns:vm="clr-namespace:TechdioApp.Pages"
             xmlns:pages="clr-namespace:TechdioApp.Dashboard.Pages"
             Shell.NavBarIsVisible="True">


    <TabBar>
    <Tab Title="Home" Icon="home.png">
            <ShellContent ContentTemplate="{DataTemplate pages:HomePage}" Route="Home" />
        </Tab>

     
        <Tab Title="Courses" Icon="courseicon.png">
            <ShellContent ContentTemplate="{DataTemplate pages:CoursePage}" Route="Courses" />
        </Tab>

     
        <Tab Title="Tutors" Icon="tutor.png">
            <ShellContent ContentTemplate="{DataTemplate pages:TutorPage}" Route="Tutors" />
        </Tab>

        <Tab Title="Profile" Icon="profile.png">
            <ShellContent ContentTemplate="{DataTemplate pages:ProfilePage}" Route="Profile" />
        </Tab>
    </TabBar>

   
    

</Shell>